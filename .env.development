# Development Environment Variables
# These values are used for local development

REACT_APP_APP_NAME=React Frontend Template
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# API Configuration
# 로컬 개발시 프록시가 /api를 제거하므로 백엔드 서버 URL만 설정
REACT_APP_API_BASE_URL=http://localhost:5000
REACT_APP_API_TIMEOUT=10000
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_AUTH_DOMAIN=dev-auth.example.com
REACT_APP_CLIENT_ID=dev-client-id
REACT_APP_REDIRECT_URI=http://localhost:3000/callback

# Feature Flags
REACT_APP_FEATURE_NEW_UI=true
REACT_APP_FEATURE_BETA_ACCESS=true
REACT_APP_FEATURE_DEBUG_MODE=true

# Development Tools
REACT_APP_MOCK_API=true
REACT_APP_LOG_LEVEL=debug

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false
FAST_REFRESH=true