# Production Environment Variables
# These values are used for production builds

REACT_APP_APP_NAME=React Frontend Template
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# API Configuration
# 프로덕션에서는 프록시 없이 직접 호출하므로 /api 포함한 전체 URL 사용
REACT_APP_API_BASE_URL=https://api.example.com/api
REACT_APP_API_TIMEOUT=5000
REACT_APP_API_VERSION=v1

# Feature Flags
REACT_APP_FEATURE_NEW_UI=false
REACT_APP_FEATURE_BETA_ACCESS=false
REACT_APP_FEATURE_DEBUG_MODE=false

# Development Tools
REACT_APP_MOCK_API=false
REACT_APP_LOG_LEVEL=error

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=true