# Staging Environment Variables
# These values are used for staging/testing environment

REACT_APP_APP_NAME=React Frontend Template
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=staging

# API Configuration
REACT_APP_API_BASE_URL=https://staging-api.example.com/api
REACT_APP_API_TIMEOUT=8000
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_AUTH_DOMAIN=staging-auth.example.com
REACT_APP_CLIENT_ID=staging-client-id
REACT_APP_REDIRECT_URI=https://staging.example.com/callback

# Feature Flags
REACT_APP_FEATURE_NEW_UI=true
REACT_APP_FEATURE_BETA_ACCESS=true
REACT_APP_FEATURE_DEBUG_MODE=false

# Development Tools
REACT_APP_MOCK_API=false
REACT_APP_LOG_LEVEL=warn

# Build Configuration
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false